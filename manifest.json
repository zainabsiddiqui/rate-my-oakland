{
  "manifest_version": 2,

  "name": "RateMyOakland",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": ["https://*.oakland.edu/*"],

      "js": ["script.js", "jquery-3.3.1.min.js"]
    }
  ],

  "permissions": [

  	"http://www.ratemyprofessors.com/*"
  ],

  "background": {

    "scripts": ["background.js"],
    "persistent": true

    }
}
