{
  "manifest_version": 2,

  "name": "RateMyOakland",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://*.oakland.edu/*"],

      "js": ["main.js", "jquery-3.3.1.js"]
    }
  ],

  "permissions": [

  	"http://www.ratemyprofessors.com/*"
  ],

  "browser_action": {
      "default_icon": {
        "16": "images/logo.png"
      }
    },

  "background": {

    "scripts": ["background.js"],
    "persistent": true

    }
}
